{
    "name": "adexaja/laravel-autodrive",
    "description": "Google api php client wrapper with Cloud Platform and Laravel 4 & 5 support With Auto Refresh Token",
    "homepage": "https://github.com/adexaja/laravel-autodrive",
    "keywords": ["laravel", "google", "cloud platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rezki Nasrullah"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "PulkitJalan\\Google\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
